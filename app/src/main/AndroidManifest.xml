<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.edu.fbv.programacaoavancada">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:hardwareAccelerated="false"
        android:largeHeap="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".CardsActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".FormularioActivity"
            android:parentActivityName=".CardsActivity" />
        <activity
            android:name=".ListProfissionalActivity"
            android:parentActivityName=".ListServiceActivity" />
        <activity
            android:name=".FeedbackActivity"
            android:parentActivityName=".CardsActivity" />
        <activity
            android:name=".ProfissionalActivity"
            android:parentActivityName=".ListProfissionalActivity" />
        <activity android:name=".OpinioesActivity" />
        <activity android:name=".ImpostoActivity" />
        <activity android:name=".ListServiceActivity"
            android:parentActivityName=".CardsActivity"></activity>
    </application>

</manifest>